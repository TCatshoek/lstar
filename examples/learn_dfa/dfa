digraph G {
	rankdir=LR
	node [shape=none]
	startz [label="" height=0 width=0]
	node [shape=circle]
	s2
	startz -> s2
	node [shape=circle]
	s1
	s2 -> s1 [label=a]
	s2 -> s2 [label=b]
	node [shape=circle]
	s1 -> s1 [label=a]
	node [shape=doublecircle]
	s3
	node [shape=circle]
	s1 -> s3 [label=b]
	node [shape=circle]
	s3 -> s3 [label=a]
	s3 -> s3 [label=b]
}
